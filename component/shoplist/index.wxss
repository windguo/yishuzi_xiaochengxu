
.list .hd {
	background: #fff;
	margin-bottom: 40rpx;
	line-height: 40rpx;
	padding:40rpx;
	display: flex;
	justify-content: space-between;
}

.list .hd .title {
	border-left: 5rpx solid orange;
	padding-left: 30rpx;
}

.list .hd .more {
	color: rgb(190, 190, 190);
}

.list .hd .more .icon-gengduo {
	font-size: 60rpx;
}

.list .bd {
	margin: 0 10px 0rpx;
	display: flex;
	justify-content: space-between;
	flex-wrap: wrap;
}

.list .bd .box {
	margin-bottom: 40rpx;
	background: #fff;
	border-radius: 10rpx;
	box-shadow: 0 2px 5px rgba(0, 0, 0, 0.05);
	width: 48%;
}

.list .bd .box .img {
	width: 100%;
	height: 500rpx;
	border-top-left-radius: 10rpx;
	border-top-right-radius: 10rpx;
}

.list .bd .box .atitle {
	padding: 10px 20rpx;
	line-height: 50rpx;
	text-align: center;
}

.list .bd .box .atitle .right {
	font-size: 24rpx;
}

.list .onclick {
	text-align: center;
	padding: 0 20rpx 15px 20rpx;
	color: #999;
}

.list .ft {
	padding: 30rpx;
	background: rgba(247, 188, 79, 0.1);
	border-top: 1rpx solid #f8f8f8;
	display: flex;
	justify-content: space-between;
}

.list .ft .price {
	font-size: 30rpx;
}

.list .ft .price .red {
	color: orangered;
	margin: 0 10rpx;
}

.list .ft .throgh {
	text-decoration: line-through;
}

.list .ft .isDoingText {
	color: orange;
}

.list .overed .price {
	color: #999;
}

.list .bd .overed .atitle .title {
	color: #999;
}

.list .box .do,
.list .overed .ov {
	z-index: 99;
	position: absolute;
	top: 130rpx;
	left: 20rpx;
	width: 150rpx;
	height: 150rpx;
}

.list .ft .tprice {
	text-decoration: line-through;
	font-size: 30rpx;
	color: #999;
}

.list .bd .overed .atitle {
	background: #f8f8f8;
}

.list .overed .intro {
	background: #ebebeb;
}

.list .overed .ft {
	background: #f1f1f1;
}

.list .overed .ft .isDoingText {
	color: #999;
}

.list .overed {
	opacity: 0.6;
}
.endtimeStyle {
	background: #f5f4f4;
	padding: 30rpx;
	display: flex;
	color: #999;
	font-size: 24rpx;
	justify-content: space-between;
}


.list .overed .endtimeStyle {
	background: rgba(0, 0, 0, 0.01);
}

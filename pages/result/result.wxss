Page{
  background: #eee;
}
.searchBox{
  line-height: 80rpx;
  margin-top:30rpx;
  padding:0 10rpx;
}
.buttonWrap{
  padding-top:30rpx;
}
.searchBox .stext {
  border:1px solid #c00;
  background:#fff;
  border-radius: 20rpx;
  padding:18rpx;
  margin-bottom:10rpx;
}
.alignCenter{
  text-align: center;
}
.alignCenter button{
  font-weight:300
}
.searchBox .sname{padding:20rpx 0;}
.searchBox .sbutton{
  margin-top: 20rpx;
}
.sharsStyle{
  margin:20rpx auto;
  text-align: center;
  width:300px;
  height:225px;
}
.btns{
  margin:50rpx 0 30rpx 0;
  display: flex;
  justify-content: space-around
}
.btns button{
  font-size: 34rpx;
}
.tips{
  background: #fffff0;
  padding:50rpx;
  margin-bottom:30rpx;
  font-size: 40rpx;
  color: brown;
}

.imagesStyle{margin: 30rpx 20rpx;background: #fff;border-radius: 20rpx;}
.imagesStyle image{width:90%;height:300rpx}
.imagesStyle h3{margin-top:10rpx;padding-bottom:50rpx;display: block;color:#999;font-size:30rpx;}

.listViews{flex:1;margin:10rpx 8rpx;background:#fff;padding:10rpx;border-radius: 10rpx;text-align: center}
.layout_vertical{
  flex-flow: row wrap;
  display: flex;
  flex-direction: row;
  margin:10rpx 20rpx 20rpx;
}

.baoxiaoListViews{margin:20rpx;background:#fff;padding:40rpx;border-radius: 20rpx;text-align: left;}
.baoxiaoListViews h3{font-size: 36rpx;display:block;padding-bottom:20rpx;font-weight: 300;line-height: 60rpx;}
.baoxiaoListViews p{font-size:30rpx;color: #666;}

swiper{height:300rpx;font-weight: 300}
.swiper_item{text-align:center;font-size:50rpx;line-height:140rpx;height:300rpx;}
swiper .img{height:300rpx;width:100%}


.wtitle{
  font-size: 40rpx;
  text-align: center;
  margin-top:20rpx;
  color: orangered;
}
.witems{
  margin:40rpx 40rpx 0;
}
.witems .img{
  margin:0 auto;
	width: 95%;
	border-radius: 20rpx;
  padding:20rpx;
  background: #fff;
	border: 1rpx solid #eee;
}


.list{
  font-size: 32rpx;
}
.list .hd {
	background: #fff;
	margin-bottom: 40rpx;
	line-height: 40rpx;
	padding: 40rpx;
	display: flex;
	justify-content: space-between;
}

.list .hd h2.title {
	border-left: 5rpx solid orange;
	padding-left: 30rpx;
}

.list .hd .more {
	color: rgb(190, 190, 190);
}

.list .hd .more .icon-gengduo {
	font-size: 60rpx;
}

.list .bd {
	margin: 0 10px 0rpx;
	display: flex;
	justify-content: space-between;
	flex-wrap: wrap;
}

.list .bd .box {
	margin-bottom: 40rpx;
	background: #fff;
	border-radius: 10rpx;
	box-shadow: 0 2px 5px rgba(0, 0, 0, 0.05);
	width: 48%;
}

.list .bd .box .img {
	width: 100%;
	height: 500rpx;
	border-top-left-radius: 10rpx;
	border-top-right-radius: 10rpx;
}

.list .bd .box .atitle {
	padding: 10px 20rpx;
	line-height: 50rpx;
	text-align: center;
}

.list .bd .box .atitle .right {
	font-size: 24rpx;
}

.list .bd .atitle span {
	color: orange;
}

.list .onclick {
	text-align: center;
	padding: 0 20rpx 15px 20rpx;
	color: #999;
}

.list .ft {
	padding: 30rpx;
	background: rgba(247, 188, 79, 0.1);
	border-top: 1rpx solid #f8f8f8;
	display: flex;
	justify-content: space-between;
}

.list .ft .price {
	font-size: 30rpx;
}

.list .ft .price span.red {
	color: orangered;
	margin: 0 10rpx;
}

.list .ft .throgh {
	text-decoration: line-through;
}

.list .ft .isDoingText {
	color: orange;
}

.list .overed .price {
	color: #999;
}

.list .bd .overed .atitle .title {
	color: #999;
}

.list .box .do,
.list .overed .ov {
	z-index: 99;
	position: absolute;
	top: 130rpx;
	left: 20rpx;
	width: 150rpx;
	height: 150rpx;
}

.list .ft .tprice {
	text-decoration: line-through;
	font-size: 30rpx;
	color: #999;
}

.list .bd .overed .atitle {
	background: #f8f8f8;
}

.list .overed .intro {
	background: #ebebeb;
}

.list .overed .ft {
	background: #f1f1f1;
}

.list .overed .ft .isDoingText {
	color: #999;
}

.list .bd .overed .atitle span {
	color: #666;
}

.list .overed {
	opacity: 0.6;
}
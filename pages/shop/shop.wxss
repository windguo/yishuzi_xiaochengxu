.wrap{background:#eee;padding-bottom:30rpx;}
.topad{text-align:center;font-size:50rpx;font-weight:300;padding:30rpx 0;line-height:70rpx;height:140rpx;background:#FF1493;color:#ffff00;}
.topad h2{display: block;}
.h2title{font-weight: 300;font-size: 50rpx;padding:40rpx 0;text-align: center;color: #FF1493;background: #ebebeb;}
.h2title:before{content:'-----';margin-right:10rpx;}
.h2title:after{content:'-----';margin-left:10rpx;}
.navigator-hover{background:transparent;}
.list{background: #fff;margin:0 50rpx 0rpx;border-radius: 20rpx;padding:50rpx;border-bottom-left-radius: 0rpx;border-bottom-right-radius: 0rpx;}
.listImg{width:100%;border-bottom:2rpx solid #f5f5f5;padding-bottom: 30rpx;}
.list h2{font-size:60rpx;}
.list ul{padding-top:20rpx;display: block;}
.list ul li{display: block;padding:10rpx 50rpx;font-size:36rpx;}
.ico_cart{width:80rpx;padding-left:70rpx;color: #FF1493}
.carts{display: flex;background: bisque;margin:0 50rpx 50rpx;border-bottom-left-radius: 20rpx;border-bottom-right-radius: 20rpx;padding:20rpx;align-items: center;}
.carts_text{line-height:90rpx;margin-left:20rpx;color: #FF1493}